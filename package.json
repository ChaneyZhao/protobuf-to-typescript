{
  "name": "pb-to-ts",
  "version": "1.0.0",
  "description": "Protobuf to TypeScript",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "files": [
    "lib",
    "libs"
  ],
  "scripts": {
    "start": "ts-node --files ./example/index",
    "watch": "rimraf ./lib && tsc -w -p tsconfig.json",
    "build": "rimraf ./lib && tsc -p tsconfig.json",
    "release": "npm run build && npm publish",
    "clean": "rm -rf node_modules"
  },
  "keywords": [
    "Protobuf",
    "TypeScript"
  ],
  "author": "chaneyzhao",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.1.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "protobufjs": "^7.2.3",
    "rimraf": "^5.0.0",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "typescript": "^4.9.4"
  }
}
